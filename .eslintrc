{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"indent": [1, "tab"],
		"no-tabs": 0,
		"semi": "off",
		"comma-dangle": "off",
		"arrow-parens": "off",
		"allowIndentationTabs": "off",
		"linebreak-style": ["error", "windows"]
	},
	"extends": ["eslint:recommended", "google"]
}
